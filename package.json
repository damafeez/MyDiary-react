{
  "name": "webpack-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest -u",
    "build:client": "webpack --mode production",
    "build:server": "babel --ignore ./server/tests -d ./dist-server ./server -s",
    "build:concurrently": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build": "rm -rf dist-client && rm -rf dist-server dist && npm run build:concurrently",
    "start:dev": "webpack-dev-server --open --mode development",
    "start": "node dist-server/index.js",
    "lint": "eslint . --fix",
    "test-watch": "jest --coverage --watchAll -u",
    "start:watch": "nodemon server/index.js --exec babel-node",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "node-sass": "^4.10.0",
    "pg": "^7.4.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-devtools": "^3.4.2",
    "redux-devtools-extension": "^2.13.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "swagger-ui-express": "^3.0.10",
    "web-push": "^3.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assetsTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/config/assetsTransformer.js"
    },
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "coverageDirectory": "coverage-jest",
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "./server"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "jest": "^23.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
